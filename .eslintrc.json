{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended","prettier","prettier/react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "semi": ["warn", "always"],
    "quotes": ["warn", "single", { "allowTemplateLiterals": true }],
    "react/jsx-uses-react": "warn",
    "react/jsx-uses-vars": "warn"
  }
}
